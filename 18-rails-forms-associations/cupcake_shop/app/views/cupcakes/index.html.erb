<h2>See all cupcakes</h2>

<%= link_to 'create a new cupcake', new_cupcake_path %>

<ul>
  <% @cupcakes.each do |cupcake| %>
    <li>
      <%= link_to cupcake.name, cupcake %>
      <%= form_tag cupcake, method: "delete", class: 'delete_btn' do %>
        <%= submit_tag "Delete Cupcake" %>
      <% end %>
    </li>
  <% end %>
</ul>
